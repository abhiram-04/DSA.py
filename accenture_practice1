def LetterNumberSum(str1):
    if str1 is None:
        return 0
    letter_values = {
        'A':1,
        'B':10,
        'C':100,
        'D':1000,
        'E':10000,
        'F':100000,
        'G':1000000
    }
    total_sum=0
    for i in str1:
        if i in letter_values.keys():
            total_sum = total_sum + letter_values[i]
    print(total_sum)   

num = input("enter the number of string: ")
string = input("Enter the string: ")
LetterNumberSum(string)
